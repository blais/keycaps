set {
  set_name: "Godspeed Custom SA Keycap Set"
  designer: "MiTo"
  manufacturer: "Signature Plastics"
  material: ABS
  stem: CHERRY
  profile: SA

  color_scheme {
    group { name: "Light Gray"   background: "WBO" }
    group { name: "Light Blue"   background: "BFJ" }
    group { name: "Light Yellow" background: "YCE" }
    group { name: "Red"          background: "RA" }
    group { name: "Cyan"         background: "VCO" }
    group { name: "Medium Gray"  background: "GTD" }
  }
}

kit_name: "Cockpit - Flat Flight Lunar Module"

# Note: This is a special "flat" kit whose profile does not match the other
# alphas kits available for this set. The keys here are mostly ROW 2, except for
# the top two rows with are ROW 2.



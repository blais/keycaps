set {
  set_name: "The Amazing Chocolatier Custom SA Keycap Set"
  designer: "Zambumon"
  manufacturer: "Signature Plastics"
  stem: CHERRY
  material: ABS
  url: "https://www.massdrop.com/buy/the-amazing-chocolatier-custom-sa-keycap-set"

  color_scheme {
    group { name: "Light brown" background: "TGH" }
    group { name: "Dark brown" background: "TBN" }
    group { name: "Yellow" background: "YCF" }
    group { name: "Red" background: "RA" }
    group { name: "White" background: "WBO" }
    group { name: "Blue" background: "BDH" }
  }
}

kit_name: "Base Kit"

# Top row.
cap { role: "grave"            text: "` ~"             size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "1"            text: "1 !"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "2"            text: "2 @"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "3"            text: "3 #"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "4"            text: "4 $"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "5"            text: "5 %"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "6"            text: "6 ^"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "7"            text: "7 &"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "8"            text: "8 *"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "9"            text: "9 ("                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "0"            text: "0 )"                 size: U100   color: "TGH" profile: SA row: R1 }
cap { role: "minus"            text: "- _"             size: U100   color: "TGH" profile: SA row: R1 }

set {
  set_name: "Hana DSA Custom Keycap Set"
  stem: CHERRY
  profile: DSA
}

kit_name: "Main set"

# Colors: RCS (pink), RCJ (purple), WFK (white)


# Function keys row.
key { role: "escape"       text: "ESC"                     size: U100   color: "RCS" }

key { role: "F1"                                           size: U100   color: "WFK" }
key { role: "F2"                                           size: U100   color: "WFK" }
key { role: "F3"                                           size: U100   color: "WFK" }
key { role: "F4"                                           size: U100   color: "WFK" }

key { role: "F5"                                           size: U100   color: "RCJ" }
key { role: "F6"                                           size: U100   color: "RCJ" }
key { role: "F7"                                           size: U100   color: "RCJ" }
key { role: "F8"                                           size: U100   color: "RCJ" }

key { role: "F9"                                           size: U100   color: "WFK" }
key { role: "F10"                                          size: U100   color: "WFK" }
key { role: "F11"                                          size: U100   color: "WFK" }
key { role: "F12"                                          size: U100   color: "WFK" }

key { role: "print"        text: "PRNT SCRN"               size: U100   color: "RCJ" }
key { role: "scroll-lock"  text: "SCRLL LOCK"              size: U100   color: "RCJ" }
key { role: "pause"        text: "PAUSE"                   size: U100   color: "RCJ" }

# Top row.
key { role: "grave"            text: "` ~"                     size: U100   color: "RCJ" }

key { role: "6"            text: "6 ^"                     size: U100   color: "WFK" }
key { role: "7"            text: "7 &"                     size: U100   color: "WFK" }
key { role: "8"            text: "8 *"                     size: U100   color: "WFK" }
key { role: "9"            text: "9 ("                     size: U100   color: "WFK" }
key { role: "0"            text: "0 )"                     size: U100   color: "WFK" }

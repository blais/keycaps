key { role: "slash"            text: "/ :"                     size: U100   color: "WFK" }
key { role: "shift"        text: "SHIFT"                   size: U275 color: "RCJ" }

# Bottom row.
key { role: "control"      text: "CTRL"                    size: U125 color: "RCJ" }

set {
  set_name: "Hana DSA Custom Keycap Set"
  profile: DSA
  stem: CHERRY

  color_scheme {
    group { name: "Pink" background: "RCS" }
    group { name: "Purple" background: "RCJ" }
    group { name: "White" background: "WFK" }
  }
}

kit_name: "Planck/Atomic"

cap { role: "tab"       text: "TAB"                     size: U100   color: "RCJ" }
cap { role: "backspace"       text: "BACK SPACE"                     size: U100   color: "RCJ" }
cap { role: "delete"       text: "DEL"                     size: U100   color: "RCJ" }
cap { role: "backslash"           text: "\\ |"                    size: U100 color: "RCJ" }
cap { role: "backslash"           text: "\\ |"                    size: U100 color: "WFK" }
cap { role: "enter"           text: "ENTER"                    size: U100 color: "RCS" }
cap { text: "LOWER"                    size: U100 color: "RCS" }
cap { text: "RAISE"                    size: U100 color: "RCS" }

cap { role: "caps-lock" text: "CAPS LOCK"                    size: U100 color: "RCJ" }
cap { role: "page-down" text: "PAGE DN"                    size: U100 color: "RCJ" }
cap { role: "page-up" text: "PAGE UP"                    size: U100 color: "RCJ" }
cap { role: "enter" text: "ENTER"                    size: U100 color: "RCJ" }
cap { role: "super" text: "SUPER"                    size: U100 color: "RCJ" }
cap { role: "function" text: "FN"                    size: U100 color: "RCJ" }
cap { text: "LOWER"                    size: U100 color: "RCJ" }
cap { text: "RAISE"                    size: U100 color: "RCJ" }

cap { role: "shift" text: "SHIFT"                    size: U100 color: "RCJ" }
cap { role: "shift" text: "SHIFT"                    size: U100 color: "RCJ" }
cap { role: "control" text: "CTRL"                    size: U100 color: "RCJ" }
cap { role: "alt" text: "ALT"                    size: U100 color: "RCJ" }
cap { role: "left" text: "LEFT"                    size: U100 color: "RCJ" }
cap { role: "down" text: "DOWN"                    size: U100 color: "RCJ" }
cap { role: "up" text: "UP"                    size: U100 color: "RCJ" }
cap { role: "right" text: "RIGHT"                    size: U100 color: "RCJ" }

cap { role: "enter" text: "ENTER"                    size: U200 color: "RCJ" }
cap { role: "shift" text: "SHIFT"                    size: U200 color: "RCJ" }
cap { text: "ha [japanese]"                    size: U100 color: "RCS" }
cap { text: "na [japanese]"                    size: U100 color: "RCS" }
cap { glyph: "flower"                    size: U100 color: "RCJ" }
cap { glyph: "flower2"                    size: U100 color: "RCJ" }

cap { glyph: "bamboo"                    size: U200 color: "RCS" }
cap { glyph: "bamboo"                    size: U200 color: "RCJ" }

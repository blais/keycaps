set {
  set_name: "Canvas"
  designer: "MiTo"
  profile: XDA
  stem: CHERRY

  color_scheme {
  }
}

kit_name: "Extra Text Modifiers"

cap { role: "enter"     glyph: "..v"  size: U100 color: "gray" }
cap { role: "shift"     glyph: "_^_"  size: U100 color: "gray" }
cap { role: "alt-graph" glyph: "^v"   size: U100 color: "gray" }
cap { role: "tab"       glyph: ".>"   size: U100 color: "gray" }
cap { role: "code"      glyph: ">_"   size: U100 color: "gray" }
cap { role: "del"       glyph: ".x"   size: U100 color: "gray" }

cap { role: "alt-graph" glyph: "^v"   size: U125 color: "gray" }
cap { role: "tab"       glyph: ".>"   size: U125 color: "gray" }
cap {                   glyph: ">_"   size: U125 color: "gray" repeat: 2 }
cap {                   glyph: ">*"   size: U125 color: "gray" repeat: 2 }

cap { role: "delete"    glyph: ".x"   size: U150 color: "gray" }
cap { role: "escape"    glyph: "`O"   size: U150 color: "gray" }
cap { role: "enter"     glyph: "..v"  size: U150 color: "gray" }

cap { role: "escape"    glyph: "`O"   size: U175 color: "gray" }
cap { role: "backspace" glyph: "..<"  size: U175 color: "gray" }
cap { role: "enter"     glyph: "..v"  size: U175 color: "gray" }
cap { role: "shift"     glyph: "_^_"  size: U175 color: "gray" }

cap { role: "shift"     glyph: "_^_t" size: U200 color: "gray" }
cap { role: "up"        glyph: "^"    size: U100 color: "gray" }
cap { role: "down"      glyph: "v"    size: U100 color: "gray" }
cap { role: "left"      glyph: "<"    size: U100 color: "gray" }
cap { role: "right"     glyph: ">"    size: U100 color: "gray" }

cap { role: "shift"     glyph: "_^_"  size: U225 color: "gray" }
cap { role: "enter"     glyph: "..v"  size: U225 color: "gray" }

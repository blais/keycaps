set {
  set_name: "Danger Zone SA Keycap Set"
  profile: SA
  stem: CHERRY
  material: ABS

  color_scheme {
    group { name: "blue" background: "BFU" }
    group { name: "grey" background: "GSM" }
    group { name: "yellow" background: "YY" }
    group { name: "red" background: "RAS" }
  }
}

kit_name: "Maverick Pack"

cap { role: "caps-lock" text: "CAPS LOCK" size: U150 color: "GSM"              row: R3 }
cap { role: "DZ*"       text: "DZ*"       size: U100 color: "GSM"              row: R3 }
cap { role: "control"   text: "CTRL"      size: U175 color: "GSM"              row: R3 }

cap { role: "h"                           size: U100 color: "BFU" homing: BUMP row: R3 }
cap { role: "u"                           size: U100 color: "BFU" homing: BUMP row: R3 }
cap { role: "n"                           size: U100 color: "BFU" homing: BUMP row: R3 }
cap { role: "t"                           size: U100 color: "BFU" homing: BUMP row: R3 }
cap { role: "function"  text: "FN"        size: U100 color: "GSM"              row: R3 }
cap { role: "shift"     text: "SHIFT"     size: U150 color: "GSM"              row: R3 }

cap { role: "control"   text: "CTRL"      size: U150 color: "GSM"              row: R3 }
cap { role: "super"     text: "SUPER"     size: U100 color: "GSM"              row: R3 }
cap { role: "alt"       text: "ALT"       size: U150 color: "GSM"              row: R3 }

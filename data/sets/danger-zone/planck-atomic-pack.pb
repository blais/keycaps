set {
  set_name: "Danger Zone SA Keycap Set"
  profile: SA_R3
  stem: CHERRY
  material: ABS

  color_scheme {
    group { name: "blue" background: "BFU" }
    group { name: "grey" background: "GSM" }
    group { name: "yellow" background: "YY" }
    group { name: "red" background: "RAS" }
  }
}

kit_name: "Planck/Atomic Pack"

cap { role: "tab"          text: "TAB"                     size: U100   color: "GSM" }
cap {                      text: "HIGH"                    size: U125 color: "GSM" }
cap {                      text: "LOW"                     size: U125 color: "GSM" }

cap { role: "backspace"    text: "BACK SPACE"              size: U100   color: "GSM" }
cap { role: "backslash"           text: "\\ |"                    size: U100   color: "GSM" }

cap { role: "caps-lock"    text: "CAPS LOCK"               size: U100   color: "GSM" }
cap {                      text: "HIGH"                    size: U100   color: "GSM" }
cap {                      text: "LOW"                     size: U100   color: "GSM" }
cap { role: "enter"        text: "ENTER"                   size: U200   color: "GSM" }
cap { role: "enter"        text: "ENTER"                   size: U100   color: "GSM" }

cap { role: "shift"        text: "SHIFT"                   size: U100   color: "GSM" }
cap {                                                      size: U200   color: "BFU" }
cap { role: "shift"        text: "SHIFT"                   size: U200   color: "GSM" }
cap { role: "shift"        text: "SHIFT"                   size: U100   color: "GSM" }

cap { role: "control"      text: "CTRL"                    size: U100   color: "GSM" }
cap { role: "win"          text: "WIN"                     size: U100   color: "GSM" }
cap { role: "alt"          text: "ALT"                     size: U100   color: "GSM" }
cap { role: "super"        text: "SUPER"                   size: U100   color: "GSM" }
cap {                      text: "HYPER"                   size: U100   color: "GSM" }
cap { role: "meta"         text: "META"                    size: U100   color: "GSM" }
